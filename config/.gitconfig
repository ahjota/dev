[user]
        name = AJ Alon
[core]
        editor = vim
[push]
        default = current
        gpgSign = if-asked
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        graph = log --graph --oneline --abbrev-commit
        addp = add -p
        last = log -1 HEAD
        rebaseia = rebase -i --autosquash
[stash]
        showPatch = true
[rebase]
        missingCommitsCheck = error
        autoSquash = true
