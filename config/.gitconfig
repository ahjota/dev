[user]
        name = AJ Alon
[core]
        editor = vim
[push]
        default = current
        gpgSign = if-asked
[alias]
        co = checkout
        ci = commit
        cp = cherry-pick
        st = status
        br = branch
        graph = log --graph --oneline --abbrev-commit
        adp = add -p
        cop = checkout -p -- .
        rhp = reset HEAD -p
        last = log -1 HEAD
        last3 = log -3 HEAD
        rebaseia = rebase -i --autosquash
[stash]
        showPatch = true
[rebase]
        missingCommitsCheck = error
        autoSquash = true
